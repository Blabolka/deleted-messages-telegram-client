{
    "API_ID": "YOUR_API_ID",
    "API_HASH": "YOUR_API_HASH",
    "notifications": {
        "consoleNotifications": {
            "enabled": true
        },
        "savedMessagesNotifications": {
            "enabled": false
        },
        "slackNotifications": {
            "enabled": false,
            "slackAccessToken": "xoxb-*your-token-from-slack-app*",
            "slackChannelId": "*channel-id-from-slack-workspace*"
        }
    },
    "features": {
        "usersWhoStartedTypingButDidNotSendIt": {
            "enabled": true
        },
        "messagesBackupsAndDeletedMessagesNotifications": {
            "enabled": true,
            "includeMutedChats": false,
            "notifications": {
                "disableNotifications": false,
                "temporaryDataStorageMaxLength": 1000
                "useMongoDatabaseAsTemporaryDataStorage": false
            }
        }
    },
    "mongoDatabase": {
        "enabled": false,
        "mongoDatabaseConnectionUrl": "*your-mongo-database-string-connection*"
    }
}
